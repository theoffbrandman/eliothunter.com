{
  "name": "eliothunter.com",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://eliothunter.com",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.28.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "shx": "^0.3.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "shx rm -rf node_modules/.cache/gh-pages && npm run build && shx cp build/index.html build/404.html",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}